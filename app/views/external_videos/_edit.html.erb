<div class="document_viewer" id="cover_container">
  <div class="multipleEdit">

    <h2 id="digital" class="section-title">Digital File</h2>
    <%= render :partial => "external_videos/edit/instantiation", :locals => {:content_type => "external_video"} %>

    <h2 id="rockhall" class="section-title">Rockhall Metadata</h2>
    <%= render :partial => "external_videos/edit/rockhall", :locals => {:content_type => "external_video"} %>

    <h2 id="permissions" class="section-title">Set permissions</h2>
    <div id="permissions">
      <%= render :partial => "permissions/index", :locals => {:document => document, :asset_id=>params[:id]} %>
    </div>

  </div>
</div>
<% sidebar_items << capture do %>

  <%= link_to("Edit original", edit_catalog_path(@document_fedora.containers.first.pid)) %>

<% end %>

