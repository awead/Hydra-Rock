<% navbar_items << capture do %>
  <%= render :partial => "catalog/previous_next_doc" %>
  <%= render :partial => "shared/navbar_partials/export_links" %>
  <%= render_workflow_menu unless params[:new_asset] %>
  <%= render_view_button %>
<% end %>

<div id="video_box" class="span6">
  <%= render_video_player %>
  <div id="video_list"><%= render_file_list %></div>
</div>

<div id="document" class="<%= render_document_class %> span6">
  <div class="well"><h1><%= @afdoc.title %></h1></div>
  <%= render_submission_workflow_step -%>
</div>